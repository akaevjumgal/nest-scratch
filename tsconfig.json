{
   "compilerOptions": {
      "lib": [
         "esnext",
         "dom"
      ],
      "baseUrl": "./",
      "target": "esnext",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./build",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "jsx": "react",
      "allowSyntheticDefaultImports": true,
      "noEmit": true,
      "types": ["node"],
      "strictPropertyInitialization": false
   }
}
